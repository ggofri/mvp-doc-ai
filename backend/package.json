{
  "name": "@fuse/backend",
  "version": "1.0.0",
  "private": true,
  "description": "Document extraction backend with Ollama",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "start": "node dist/index.js",
    "test": "vitest",
    "db:init": "ts-node -e \"import('./src/db/connection').then(c => c.initDatabase({ path: '../data/db.sqlite' })).then(() => import('./src/db/init')).then(i => i.initSchema()).then(() => console.log('Database initialized')).catch(console.error)\""
  },
  "keywords": [
    "document-extraction",
    "ollama",
    "ocr"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.58",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^2.0.0",
    "pdfjs-dist": "^4.8.69",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^5.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.2",
    "vitest": "^3.0.1"
  }
}
